    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/tang",
            "args": [
                "-s",
                "text.tang",
                "-o",
                "a"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug",
            "program": "${workspaceFolder}/tang",
            "args": [
                "-s",
                "test.tang",
                "-o",
                "a"
            ],
            "cwd": "${workspaceFolder}",
        }
    ]
}